{
    "name": "blackknight467/ayah-bundle",
    "type": "symfony-bundle",
    "description": "Are you a human bundle for use with Symfony 2.  This bundle adds the 'ayah' form type.  Simple and easy to use.  Lets leave the captcha behind",
    "keywords": ["are", "you", "a", "human", "ayah", "bundle", "symfony", "symfony2", "captcha", "security", "areyouahuman", "are you a human"],
    "homepage": "http://github.com/blackknight467/AYAHBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Derick Felsman",
            "email": "derickfelsman@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "symfony/dependency-injection": "~2.3|~3.0",
        "symfony/config": "~2.3|~3.0",
        "symfony/options-resolver": "~2.7|~3.0",
        "symfony/property-access": "~2.3|~3.0",
        "symfony/http-kernel": "~2.3|~3.0",
        "symfony/form": "~2.3|~3.0"
    },
    "autoload": {
        "psr-0": { "blackknight467\\AYAHBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "blackknight467/AYAHBundle"
}
